    <style>
    .container {
        background-color: coral;
        display: grid;
        /*width: 175vh;
        height: auto;*/
        margin: 0px auto;
        grid-template-columns: repeat(2, 1fr);
        grid-gap: 50px;

        & h2 {
            background-color: brown;
            font-family:'Quicksand';
            margin: 0px;
        }
        & p {
            background-color: rgb(42, 165, 42);
            font-family:'Quicksand';
            margin: 0px;
        }
    }

    .bento {
        background-color: #d0d312;
        display: grid;
        width: 100vh;
        height: 100vh;
        margin: 0 auto;
        grid-template-columns: repeat(4, 1fr);
        grid-template-rows: repeat(4, 1fr);
        grid-gap: 20px;
    }

    .item {
        color: white;
        font-size: 20px;
        align-items: end;
        display:flex;
        flex-direction: row-reverse;

        & p {
            background-color: #0000007e;
            margin: 10px;
            padding: 10px;
            border-radius: 5px;
        }
    }

    .item-1 {
        background-image: url(/home/veronica/workspace/justsetgo-landing/idea-montaje/imagenes/viajes1/bali.jpeg);
        grid-column: 1 / span 2;
        grid-row: 1 / span 2;
    }
    .item-2 {
        background: #33a8a5;
    }
    .item-3 {
        background: #30997a;
    }
    .item-4 {
        background-image: url(/home/veronica/workspace/justsetgo-landing/idea-montaje/imagenes/viajes1/paris.jpeg);
        background-size: cover;
        grid-column: 4;
        grid-row: 1 / span 2;
    }
    .item-5 {
        background-image: url(/home/veronica/workspace/justsetgo-landing/idea-montaje/imagenes/BGs/BG3.jpg);
        background-size: cover;
    }
    .item-6 {
        background: #3d8bb1;
    }
    .item-7 {
        background: #e03f3f;
        grid-column: 1 / span 2;
        grid-row: 4;
    }
    .item-8 {
        background-image: url(/home/veronica/workspace/justsetgo-landing/idea-montaje/imagenes/viajes1/roma.jpeg);
        background-size: cover;
        grid-column: 3;
        grid-row: 3 / span 2;
    }
    .item-9 {
        background: #4464a1;
    }
    .item-10 {
        background: palevioletred;
    }
</style>

